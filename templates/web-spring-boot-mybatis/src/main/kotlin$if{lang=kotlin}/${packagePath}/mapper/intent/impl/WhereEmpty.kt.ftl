package ${packageName}.mapper.intent.implimport ${packageName}.mapper.intent.api.OrderQueryimport ${packageName}.mapper.intent.api.WhereQueryclass WhereEmpty<T> : WhereItem<T>(), WhereQuery<T> {    override fun or(where: WhereQuery<T>): WhereQuery<T> {        return where    }    override fun and(where: WhereQuery<T>): WhereQuery<T> {        return where    }    override fun orderBy(vararg orders: OrderQuery<T>): OrderQuery<T> {        return orders.reduce { l, r -> l.orderBy(r) }    }}