package ${packageName}.mapper.intent.impl;import ${packageName}.mapper.intent.api.Query;import java.util.LinkedList;import java.util.List;public class OrderQuery<T> implements Query<T> {    public final String column;    public final boolean desc;    public final List<OrderQuery<T>> orders = new LinkedList<>();    public OrderQuery(String column, boolean desc) {        this.desc = desc;        this.column = column;        this.orders.add(this);    }    public OrderQuery<T> orderBy(OrderQuery<T> order) {        orders.add(order);        return this;    }    @Override    public List<OrderQuery<T>> getOrders() {        return orders;    }}