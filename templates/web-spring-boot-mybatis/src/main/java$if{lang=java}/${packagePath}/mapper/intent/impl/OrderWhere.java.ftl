package ${packageName}.mapper.intent.impl;import java.util.Arrays;import java.util.LinkedList;import java.util.List;public class OrderWhere<T> extends WhereItem<T> {    public final List<Where<T>> wheres;    public final List<OrderQuery<T>> orders = new LinkedList<>();    public OrderWhere(Where<T> where) {        super(where);        this.wheres = where.getWheres();    }    @SafeVarargs    public final OrderWhere<T> orderBy(OrderQuery<T>... order) {        orders.addAll(Arrays.asList(order));        return this;    }    @Override    public List<OrderQuery<T>> getOrders() {        return orders;    }    @Override    public List<Where<T>> getWheres() {        return wheres;    }}